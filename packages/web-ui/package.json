{
  "name": "web-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --import ./scripts/node-websocket-polyfill.mjs ../../node_modules/vite/bin/vite.js",
    "build": "pnpm exec pnpm exec tsc -b && vite build",
    "start": "node scripts/start.mjs",
    "typecheck": "pnpm exec pnpm exec tsc --noEmit",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.0.6",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^17.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": "20.x",
    "pnpm": "9.x"
  },
  "packageManager": "pnpm@9.15.0"
