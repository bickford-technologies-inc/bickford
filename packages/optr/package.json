{
  "name": "@bickford/optr",
  "version": "0.0.0",
  "description": "OPTR scoring and non-interference evaluation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "cli"
  ],
  "bin": {
    "optr-execute": "cli/execute.js"
  },
  "scripts": {
