    "lint": "npm run typecheck",
    "test": "echo 'No tests yet'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bickford",
    "optr",
    "non-interference"
  ],
  "author": "Bickford",
  "license": "PROPRIETARY",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@bickford/types": "workspace:*",
    "@octokit/rest": "^22.0.1",
    "openai": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "typescript": "^5.0.0"
  },
  "type": "module"
}
