{
  "expo": {
    "name": "bickford",
    "slug": "bickford",
    "version": "0.1.0",
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "platforms": ["ios"],
    "ios": {
      "supportsTablet": false,
      "requireFullScreen": true,
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.bickfordd.bickford",
      "buildNumber": "1",
      "infoPlist": {
        "UIDeviceFamily": [1],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false
        }
      }
    },
    "extra": {
      "crashReporting": {
        "enabled": true
      }
    }
  }
}
