{
  "cli": {
    "version": ">= 8.0.0"
  },
  "build": {
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "distribution": "store"
      }
    },
    "staging": {
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "distribution": "internal"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "${APPLE_ID}",
        "ascAppId": "${ASC_APP_ID}",
        "appleTeamId": "${APPLE_TEAM_ID}"
      }
    }
  }
}
