{
  "name": "bickford-mobile-expo",
  "version": "0.1.0",
  "private": true,
  "description": "Bickford Mobile (Expo/EAS) native wrapper",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "icon:generate": "node scripts/generate-icon.mjs",
    "postinstall": "npm run icon:generate",
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "prebuild": "expo prebuild --clean",
    "eas:build:ios": "eas build --platform ios --profile production",
    "eas:submit:ios": "eas submit --platform ios --profile production",
    "lint": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "expo": "~54.0.30",
    "react": "^19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
