{
  "name": "@bickford/session-completion",
  "version": "0.0.0",
  "description": "Session completion event capture, validation, and routing runtime",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "echo 'Use npm run dev from root to start the web app'",
    "start": "echo 'Use npm start from root to start the web app'",
    "build": "pnpm exec pnpm exec tsc",
    "typecheck": "pnpm exec pnpm exec tsc --noEmit",
    "lint": "npm run typecheck",
    "example:capture": "tsx examples/capture-demo.mjs",
    "test": "echo 'No tests yet'"
  },
  "keywords": [
    "session-completion",
    "event-capture",
    "ai-agents",
    "runtime",
    "routing"
  ],
  "author": "Bickford",
  "license": "PROPRIETARY",
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@bickford/chat": "workspace:*"
  }
}
