{
  "name": "bickford-mobile-ui",
  "version": "0.1.0",
  "private": true,
  "description": "Mobile PWA UI for Bickford OPTR TV2 (React + Vite + PWA)",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:native": "expo prebuild",
    "start": "vite preview",
    "preview": "vite preview",
    "lint": "pnpm exec pnpm exec tsc -p tsconfig.json",
    "eas:build:android": "eas build --platform android --profile production",
    "eas:build:ios": "eas build --platform ios --profile production",
    "eas:submit:android": "eas submit --platform android --profile production",
    "eas:submit:ios": "eas submit --platform ios --profile production",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "echo 'No tests yet'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.7",
    "@rolldown/pluginutils": "^1.0.0-beta.27",
    "expo": "~54.0.30",
    "react-native": "0.83.1",
    "rollup": "^4.55.1",
    "serialize-javascript": "^6.0.2",
    "smob": "^1.5.0",
    "upath": "^1.2.0"
  },
  "devDependencies": {
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.0.0",
    "is-module": "^1.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "0.19.8"
  }
}
