{
  "name": "session-completion-runtime-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "$25M acquisition package: Session completion runtime + Bickford Canon",
  "scripts": {
    "demo:a": "tsx demo/demo-a.ts",
    "demo:c": "tsx demo/demo-c.ts",
    "demo:d": "tsx demo/demo-d.ts",
    "demo:optr-deal": "tsx demo/demo-optr-deal.ts",
    "demo:video": "tsx demo/generate-video.ts",
    "example:optr": "cd packages/bickford && npm run example:optr",
    "example:capture": "cd packages/session-completion-runtime && npm run example:capture",
    "install:all": "npm install --workspaces",
    "dev": "concurrently -n API,WEB,MOBILE -c auto \"npm run dev:api\" \"npm run dev:web\" \"npm run dev:mobile\"",
    "dev:api": "npm -w packages/session-completion-runtime run dev",
    "dev:web": "npm -w packages/demo-dashboard run dev",
    "dev:mobile": "npm -w packages/bickford-mobile-ui run dev",
    "build": "npm --workspaces run build",
    "build:api": "npm -w packages/session-completion-runtime run build",
    "build:web": "npm -w packages/demo-dashboard run build",
    "build:mobile": "npm -w packages/bickford-mobile-ui run build",
    "lint": "npm --workspaces --if-present run lint",
    "test": "npm --workspaces run test",
    "bid:prep": "tsx scripts/bid/prepare-bid-pack.ts --config bid/bid.config.json",
    "bid:prep:out": "tsx scripts/bid/prepare-bid-pack.ts --config bid/bid.config.json --out bid_out",
    "bid:b1": "tsx scripts/bid/generate-b1-packages.ts --list bid/b1.opportunities.json --out out/bid_packages_b1",
    "sam:search": "tsx scripts/samgov/search-opportunities.ts",
    "sam:fetch:notice": "tsx scripts/samgov/fetch-notice.ts",
    "bid:enrich:samgov": "tsx scripts/samgov/enrich-bid-out.ts",
    "sam:score": "tsx scripts/samgov/score-opportunities.ts",
    "sam:outbox": "tsx scripts/samgov/render-outbox.ts"
    ,
    "complete": "make complete"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "expo": "^54.0.30",
    "expo-dev-client": "^6.0.20",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "redis": "^5.10.0",
    "zod": "^4.2.1"
  }
}
