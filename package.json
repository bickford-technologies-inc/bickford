{
  "name": "bickford",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "start": "bash scripts/setup.sh && npm run dev",
    "dev": "pnpm pnpm exec prisma:generate && pnpm exec pnpm exec turbo run dev --filter=web",
    "check:turbo": "node scripts/check-pnpm exec pnpm exec turbo-extends.mjs",
    "build": "turbo run build",
    "build:strict": "pnpm --filter @bickford/db exec pnpm exec prisma generate && pnpm exec pnpm exec turbo run build",
    "prisma:generate": "pnpm --filter @bickford/db exec pnpm exec prisma generate",
    "prisma:migrate:dev": "pnpm exec pnpm exec prisma migrate dev --schema=pnpm exec prisma/schema.pnpm exec prisma",
    "prisma:migrate:deploy": "pnpm exec pnpm exec prisma migrate deploy --schema=pnpm exec prisma/schema.pnpm exec prisma",
    "prisma:migrate": "pnpm exec pnpm exec prisma migrate deploy",
    "db:migrate": "pnpm exec pnpm exec prisma migrate dev",
    "db:push": "pnpm exec pnpm exec prisma db push",
    "test": "pnpm exec pnpm exec turbo run test",
    "deploy": "vercel --prod",
    "setup": "node scripts/setup.mjs",
    "setup:interactive": "node scripts/setup-env.mjs",
    "setup:bash": "bash scripts/setup-env.sh",
    "demo:a": "tsx demo/demo-a.mjs",
    "demo:c": "tsx demo/demo-c.mjs",
    "demo:d": "tsx demo/demo-d.mjs",
    "demo:closures": "tsx demo/demo-closures.mjs",
    "demo:trust-ux": "tsx demo/demo-trust-ux.mjs",
    "demo:optr-deal": "tsx demo/demo-optr-deal.mjs",
    "demo:video": "tsx demo/generate-video.mjs",
    "demo:ledger": "tsx demo/demo-ledger.mjs",
    "example:optr": "cd packages/bickford && npm run example:optr",
    "example:capture": "cd packages/session-completion-runtime && npm run example:capture",
    "install:all": "npm install --workspaces",
    "install:ci": "npm ci --workspaces",
    "postinstall": "node -e \"const cp=require('node:child_process'); if(process.env.VERCEL||process.env.CI){console.log('postinstall: skipping monorepo build on CI/Vercel');} else {cp.execSync('npm run build',{stdio:'inherit'});}\"",
    "postinstall:prisma-fix": "node scripts/fix-pnpm exec prisma-imports.mjs",
    "dev:old": "concurrently -n API,WEB,MOBILE -c auto \"npm run dev:api\" \"npm run dev:web\" \"npm run dev:mobile\"",
    "dev:api": "npm -w packages/session-completion-runtime run dev",
    "dev:web": "npm -w packages/web-ui run dev",
    "dev:mobile": "npm -w packages/bickford-mobile-ui run dev",
    "dev:chat": "node scripts/run-chat-stack.mjs",
    "build:types": "npm -w packages/types run build",
    "build:api": "npm -w packages/session-completion-runtime run build",
    "build:web": "npm -w packages/web-ui run build",
    "build:mobile": "npm -w packages/bickford-mobile-ui run build",
    "build:all": "pnpm -w packages/types run build && pnpm -w packages/optr run build && pnpm -w packages/authority run build && pnpm -w packages/core run build && pnpm -w packages/canon run build && pnpm -w packages/ledger run build && pnpm -w packages/session-completion run build && pnpm -w packages/ui run build && pnpm -w packages/web-ui run build && pnpm -w packages/claude-integration run build && pnpm -w packages/bickford-mobile-expo run build",
    "smoke": "node scripts/run-smoke.mjs",
    "ci:smoke": "cross-env CI=1 npm run smoke",
    "quickstart": "npm run smoke",
    "derek": "npm run smoke",
    "codespaces:url:web": "node scripts/print-codespaces-url.mjs",
    "git:sync": "node scripts/git-sync.mjs",
    "git:sync:watch": "node scripts/git-sync-daemon.mjs",
    "lint": "npm --workspaces run lint",
    "typecheck": "npm --workspaces --if-present run typecheck",
    "check": "bash scripts/health-check.sh",
    "check:invariants": "node scripts/check-invariants.mjs",
    "check:chat": "node scripts/check-chat-invariants.mjs",
    "check:canon": "node scripts/check-canon-invariants.mjs",
    "check:no-deep-imports": "node scripts/check-no-deep-imports.mjs",
    "check:all": "npm run check:invariants && npm run check:chat && npm run check:canon && npm run check:no-deep-imports",
    "check:prisma-canon": "node scripts/check-pnpm exec prisma-canon.mjs",
    "check:esm": "node scripts/check-esm-invariants.mjs",
    "deploy:old": "bash scripts/deploy.sh",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "deploy:production": "bash scripts/deploy.sh production",
    "lint:fix": "npm --workspaces --if-present run lint:fix",
    "fix-all": "npm run lint:fix && bash scripts/auto-recover.sh",
    "prepare": "node -e \"if(process.env.CI||process.env.VERCEL){console.log('prepare: skipping husky in CI/Vercel')}else{try{require('node:child_process').execSync('husky',{stdio:'inherit'})}catch(e){}}\"",
    "restart:markitdown": "node scripts/restart-markitdown.mjs",
    "test:chat": "node scripts/test-chat.mjs",
    "architect:inventory": "node scripts/inventory-repos.mjs",
    "architect:prime": "node scripts/architect-prime.mjs",
    "setup:railway": "./scripts/setup-railway.sh",
    "deploy:railway": "railway up",
    "logs:railway": "railway logs",
    "railway:doctor": "node scripts/railway-doctor.mjs",
    "railway:deploy:web": "railway up -s web-ui -d",
    "env:vercel": "node scripts/provision-vercel-env.mjs",
    "doctor": "sh scripts/bickford-doctor.sh",
    "doctor:fix": "sh scripts/bickford-doctor-fix.sh",
    "doctor:ci": "node scripts/doctor-ci.mjs",
    "resolve:push": "node ./scripts/resolve-and-push.mjs",
    "verify:next": "tsx scripts/verify-next-version.mjs",
    "prebuild": "bash ./ci/guards/prebuild-invariant.sh",
    "ci:verify:turbo": "tsx scripts/ci/verify-pnpm exec pnpm exec turbo-ordering.mjs",
    "predictive:review": "tsx scripts/predictive/review.mjs",
    "predictive:scan": "tsx scripts/predictive/scan-runtime-boundaries.mjs",
    "predictive:autofix": "tsx scripts/predictive/autofix.mjs",
    "ci:prisma-guard": "node ci/check-no-pnpm exec prisma-in-api-routes.mjs",
    "verify:prisma-boundary": "node ci/check-pnpm exec prisma-boundary.mjs",
    "verify:runtime-purity": "node ci/check-runtime-purity.mjs",
    "preinstall": "bash ./ci/guards/TOOLCHAIN_GUARD.sh"
  },
  "dependencies": {
    "@vercel/sdk": "^1.0.0"
  },
  "devDependencies": {
    "prisma": "6.19.2",
    "@prisma/client": "6.19.2",
    "tsx": "^4.7.0",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "ajv": "^8.12.0",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": "20.x"
  }
}
