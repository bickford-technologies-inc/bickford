{
  "name": "bickford",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.8.2",
  "description": "Intent â†’ Reality in <5 seconds. Zero-approval execution runtime with OPTR gating, canon enforcement, and immutable ledger.",
  "engines": {
    "node": ">=20.19.4",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "bash scripts/setup.sh && npm run dev",
    "dev": "turbo run dev --filter=web",
    "build": "turbo run build",
    "test": "turbo run test",
    "deploy": "vercel --prod",
    "prisma:migrate": "prisma migrate deploy",
    "start:dev": "bash scripts/start.sh",
    "start:safe": "bash scripts/auto-recover.sh && npm start",
    "recover": "bash scripts/auto-recover.sh",
    "health": "bash scripts/health-check.sh",
    "health:vercel": "node scripts/health-check-vercel.mjs",
    "monitor": "bash scripts/health-monitor.sh",
    "diagnose": "bash scripts/diagnose.sh",
    "setup": "node scripts/setup.mjs",
    "setup:interactive": "node scripts/setup-env.mjs",
    "setup:bash": "bash scripts/setup-env.sh",
    "demo:a": "tsx demo/demo-a.ts",
    "demo:c": "tsx demo/demo-c.ts",
    "demo:d": "tsx demo/demo-d.ts",
    "demo:optr-deal": "tsx demo/demo-optr-deal.ts",
    "demo:video": "tsx demo/generate-video.ts",
    "demo:ledger": "tsx demo/demo-ledger.ts",
    "example:optr": "cd packages/bickford && npm run example:optr",
    "example:capture": "cd packages/session-completion-runtime && npm run example:capture",
    "install:all": "npm install --workspaces",
    "install:ci": "npm ci --workspaces",
    "postinstall": "node -e \"const cp=require('node:child_process'); if(process.env.VERCEL||process.env.CI){console.log('postinstall: skipping monorepo build on CI/Vercel');} else {cp.execSync('npm run build',{stdio:'inherit'});}\"",
    "dev:old": "concurrently -n API,WEB,MOBILE -c auto \"npm run dev:api\" \"npm run dev:web\" \"npm run dev:mobile\"",
    "dev:api": "npm -w packages/session-completion-runtime run dev",
    "dev:web": "npm -w packages/web-ui run dev",
    "dev:mobile": "npm -w packages/bickford-mobile-ui run dev",
    "dev:chat": "node scripts/run-chat-stack.mjs",
    "build:old": "npm run build:types && prisma generate && npm run build:api && npm run build:web && npm run build:mobile",
    "build:types": "npm -w packages/types run build",
    "build:api": "npm -w packages/session-completion-runtime run build",
    "build:web": "npm -w packages/web-ui run build",
    "build:mobile": "npm -w packages/bickford-mobile-ui run build",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "smoke": "node scripts/run-smoke.mjs",
    "ci:smoke": "cross-env CI=1 npm run smoke",
    "quickstart": "npm run smoke",
    "derek": "npm run smoke",
    "codespaces:url:web": "node scripts/print-codespaces-url.mjs",
    "git:sync": "node scripts/git-sync.mjs",
    "git:sync:watch": "node scripts/git-sync-daemon.mjs",
    "lint": "npm --workspaces run lint",
    "typecheck": "npm --workspaces --if-present run typecheck",
    "check": "bash scripts/health-check.sh",
    "deploy:old": "bash scripts/deploy.sh",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "deploy:production": "bash scripts/deploy.sh production",
    "lint:fix": "npm --workspaces --if-present run lint:fix",
    "fix-all": "npm run lint:fix && bash scripts/auto-recover.sh",
    "prepare": "node -e \"if(process.env.CI||process.env.VERCEL){console.log('prepare: skipping husky in CI/Vercel')}else{try{require('node:child_process').execSync('husky',{stdio:'inherit'})}catch(e){}}\"",
    "restart:markitdown": "node scripts/restart-markitdown.mjs",
    "test:chat": "node scripts/test-chat.mjs",
    "architect:inventory": "node scripts/inventory-repos.mjs",
    "architect:prime": "node scripts/architect-prime.mjs",
    "setup:railway": "./scripts/setup-railway.sh",
    "deploy:railway": "railway up",
    "logs:railway": "railway logs",
    "railway:doctor": "node scripts/railway-doctor.mjs",
    "railway:deploy:web": "railway up -s web-ui -d"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@netlify/plugin-lighthouse": "^6.0.1",
    "@prisma/client": "^7.2.0",
    "@sentry/netlify-build-plugin": "^1.1.1",
    "@snaplet/netlify-preview-database-plugin": "^2.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "netlify-plugin-checklinks": "^4.1.1",
    "netlify-plugin-contextual-env": "^0.3.0",
    "netlify-plugin-debug-cache": "^1.0.4",
    "netlify-plugin-prisma-provider": "^0.3.0",
    "netlify-plugin-visual-diff": "^2.0.0",
    "prisma": "^7.2.0",
    "ts-jest": "^29.2.5",
    "turbo": "^2.7.4",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "@octokit/rest": "^22.0.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "redis": "^5.10.0",
    "zod": "^4.3.5"
  }
}
