    "pdf-lib": "^1.17.1",
    "prisma": "7.3.0",
    "react-markdown": "^10.1.0",
    "swr": "^2.4.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "next": "15.5.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
