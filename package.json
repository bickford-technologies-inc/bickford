{
  "name": "bickford",
  "private": true,
  "scripts": {
    "dev": "bun run next dev",
    "build": "bun run next build",
    "start": "bun run next start",
    "typecheck": "bun run tsc --noEmit --project tsconfig.json",
    "prebuild": "bun run tsx scripts/validate-repo-contents.ts",
