    "execute:technical-automation": "bun run tsx scripts/execute-technical-automation.ts",
    "print:sam-gov-data": "bun run scripts/print-sam-gov-data.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
