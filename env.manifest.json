{
  "shared": {
    "DATABASE_URL": {
      "value": "${{ Clickhouse.DATABASE_URL }}",
      "sealed": true,
      "env": ["prod", "staging"]
    },
    "API_KEY": {
      "value": "REDACTED",
      "sealed": true,
      "env": ["prod"]
    },
    "LOG_LEVEL": {
      "value": "info",
      "sealed": false,
      "env": ["all"]
    }
  },
  "services": {
    "backend": {
      "API_KEY": {
        "value": "${{ shared.API_KEY }}",
        "sealed": false,
        "env": ["prod"]
      },
      "DATABASE_URL": {
        "value": "${{ shared.DATABASE_URL }}",
        "sealed": false,
        "env": ["prod", "staging"]
      }
    },
    "frontend": {
      "API_URL": {
        "value": "https://${{ backend.RAILWAY_PUBLIC_DOMAIN }}",
        "sealed": false,
        "env": ["all"]
      }
    }
  }
}
