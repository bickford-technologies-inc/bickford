{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        ".next/**"
      ]
    },
    "clean": {
      "outputs": []
    },
    "db:generate": {
      "outputs": [
        "packages/db/node_modules/.prisma/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_URL",
        "NODE_ENV"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "env": [
        "DATABASE_URL",
        "NODE_ENV",
        "CI"
      ]
    },
    "typecheck": {
      "outputs": []
    },
    "prisma:generate": {
      "outputs": ["packages/ledger/node_modules/@prisma/client/**"]
    },
    "prebuild": {
      "outputs": []
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "OPENAI_API_KEY",
    "ANTHROPIC_API_KEY",
    "NODE_ENV",
    "VERCEL",
    "CI",
    "NEXT_RUNTIME",
    "VERCEL_EDGE",
    "BICKFORD_API_TOKEN",
    "GITHUB_TOKEN",
    "PG_SSL"
  ]
}